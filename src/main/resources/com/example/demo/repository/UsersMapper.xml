<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.UsersMapper">

<resultMap type="com.example.demo.entity.Users" id="celebratedPersons">
	<id column="user_id" property="userId" />
	<result column="mail_address" property="mailAddress" />
	<result column="password" property="password" />
	<result column="name" property="name" />
	<result column="birth_date" property="birthDate" />
	<result column="age" property="age" />
	<result column="role" property="role" />
</resultMap>

<insert id="signUp">
	INSERT INTO users (mail_address, password, name, birth_date, age) VALUES (#{mailAddress}, #{password}, #{name},#{birthDate}, #{age}) 
</insert>

</mapper>